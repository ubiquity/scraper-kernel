{
  "name": "@ubiquity/scraper-kernel",
  "license": "MIT",
  "author": {
    "name": "アレクサンダー.eth"
  },
  "main": "dist/scrape.js",
  "module": "dist/scrape.js",
  "types": "dist/scrape.d.ts",
  "version": "0.1.0",
  "scripts": {
    "utils:database-export": "node dist/utils/github-api-enrichment/export-database",
    "prepare": "husky install",
    "prebuild": "run-s clean",
    "build": "tsc --watch",
    "clean": "rimraf dist tsconfig.tsbuildinfo node_modules",
    "format": "run-s format:*",
    "format:prettier": "prettier --write .",
    "format:eslint": "eslint . --ext .ts --fix"
  },
  "lint-staged": {
    "*.{ts}": "eslint --fix"
  },
  "nodemonConfig": {
    "watch": "tsconfig.tsbuildinfo",
    "ignore": [
      "dist/*.json",
      "**/*/cache/",
      "**/*/artifacts/",
      "**/*/node_modules/**/*"
    ],
    "events": {
      "config": "",
      "exit": "",
      "crash": "",
      "restart": "clear && printf '\\033[3J'",
      "start": "clear && printf '\\033[3J'"
    }
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/follow-redirects": "^1.14.1",
    "@types/node": "^18.11.18",
    "@types/puppeteer": "^7.0.4",
    "@types/source-map-support": "^0.5.6"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsc": "^2.0.0",
    "follow-redirects": "^1.15.2",
    "husky": "^8.0.3",
    "json2csv": "^6.0.0-alpha.2",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "puppeteer": "^19.6.2",
    "source-map-support": "^0.5.21",
    "tsx": "^3.12.2",
    "typescript": "^4.9.5",
    "yarn-upgrade-all": "^0.7.2"
  }
}
